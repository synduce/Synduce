(set-logic LIA)
(define-fun max ((x Int) (y Int)) Int (ite (>= x y) x y))
(synth-fun join ((x1 Int) (x2 Int) (x3 Int) (x4 Int)) Int ((IStart Int) (Ix Int) (Ic Int))
 ((IStart Int ((max Ix Ix))) (Ix Int (Ic x1 x2 x3 x4 (- Ix) (+ Ix Ix) (max Ix Ix)))
  (Ic Int ((Constant Int)))))
(declare-var i4 Int)
(declare-var i5 Int)
(declare-var i0 Int)
(declare-var i Int)
(constraint (= (max i (max i0 i5)) (join i i0 0 i5)))
(constraint (= (max i4 (max i (max i0 i5))) (join i i0 (max i4 0) i5)))
(check-synth)
