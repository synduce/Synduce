(set-logic LIA)
(synth-fun join ((x1 Int) (x2 Int) (x3 Int) (x4 Int)) Int ((IStart Int) (Ix Int) (Ic Int))
 ((IStart Int ((+ Ix Ix))) (Ix Int (Ic x1 x2 x3 x4 (- Ix) (+ Ix Ix))) (Ic Int ((Constant Int)))))
(declare-var i1 Int)
(declare-var i2 Int)
(declare-var p1 Int)
(declare-var p0 Int)
(constraint (= (+ p0 (+ p1 i2)) (join p0 p1 0 i2)))
(constraint (= (+ i1 (+ p0 (+ p1 i2))) (join p0 p1 (+ i1 0) i2)))
(check-synth)
