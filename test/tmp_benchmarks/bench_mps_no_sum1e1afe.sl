(set-logic LIA)
(define-fun max ((x Int) (y Int)) Int (ite (>= x y) x y))
(synth-fun oplus ((x4 Int) (x5 Int)) Int ((IStart Int) (Ix Int) (Ic Int))
 ((IStart Int ((max Ix Ix))) (Ix Int (Ic x4 x5 (- Ix) (+ Ix Ix) (max Ix Ix)))
  (Ic Int ((Constant Int)))))
(declare-var p1 Int)
(declare-var p Int)
(constraint (= (max p 0) (oplus 0 p)))
(constraint (= (max (+ (max p1 0) p) 0) (oplus (max p 0) p1)))
(check-synth)
