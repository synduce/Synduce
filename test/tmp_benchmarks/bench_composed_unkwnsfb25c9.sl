(set-logic LIA)
(synth-fun f2 ((x1 Int)) Int)
(synth-fun f1 ((x2 Int)) Int)
(synth-fun join ((x3 Int) (x4 Int)) Int ((IStart Int) (Ix Int) (Ic Int))
 ((IStart Int ((+ Ix Ix))) (Ix Int (Ic x3 x4 (- Ix) (+ Ix Ix))) (Ic Int ((Constant Int)))))
(declare-var i Int)
(declare-var i0 Int)
(constraint (= i0 (join (f1 0) (f2 i0))))
(constraint (= (+ i i0) (join (f1 (+ i 0)) (f2 i0))))
(check-synth)
