(set-logic LIA)
(define-fun max ((x Int) (y Int)) Int (ite (>= x y) x y))
(synth-fun xi_1 ((x1 Int) (x2 Int)) Int ((IStart Int) (Ix Int) (Ic Int))
 ((IStart Int ((max Ix Ix))) (Ix Int (Ic x1 x2 (- Ix) (+ Ix Ix) (max Ix Ix)))
  (Ic Int ((Constant Int)))))
(declare-var i1 Int)
(declare-var i0 Int)
(declare-var p Int)
(constraint (= (max i0 (max p i1)) (xi_1 p i0)))
(check-synth)
