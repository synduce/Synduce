(set-logic LIA)
(define-fun min ((x Int) (y Int)) Int (ite (<= x y) x y))
(synth-fun f0 ((x1 Int) (x2 Int)) Int ((IStart Int) (Ix Int) (Ic Int))
 ((IStart Int ((min Ix Ix))) (Ix Int (Ic x1 x2 (- Ix) (+ Ix Ix) (min Ix Ix)))
  (Ic Int ((Constant Int)))))
(declare-var i2 Int)
(declare-var i1 Int)
(declare-var i Int)
(constraint (= (min i (min i1 i2)) (f0 i i1)))
(check-synth)
